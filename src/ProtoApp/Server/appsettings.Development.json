{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information",
      "Microsoft.EntityFrameworkCore.Model.Validation": "Warning",
      "Tlabs": "Debug"
    },
    "configurator": {
      "stdoutLogger": {
        "options": {
          "includeCategory": true
        }
      }
    }
  },
  // "webHosting": {
  //   "webroot": "../../rsc"
  // },
  "applicationMiddleware": {
    "debugPages": {
      "ord": 20,
      "type": "Tlabs.Config.DebugPagesConfigurator, Tlabs.SrvBase"
    },
    "CORS": {
      "ord": 21,
      "type": "Tlabs.Config.CorsConfigurator, Tlabs.SrvBase",
      "options": {
        "AllowAnyHeader": true,
        "AllowAnyMethod": true,
        "AllowCredentials": true,
        "AllowedOrigins": ["http://localhost:8080", "http://localhost:8001"]
      }
    }
  },
  "applicationServices": {
    "ASPNET.MVC": {
      // "type": "Tlabs.Config.MvcSvcConfigurator, Tlabs.SrvBase",
      // "config": {
      //   "001_filterApiKeyAuth": null, //remove filter
      //   "002_filterUserAuth": null, //remove filter
      //   "formatting": "Indented" //"None"
      // }
    }
  }
}
